pwd;\r\n              var a =this.uage;\r\n              console.log(u,p,a);\r\n              //验证用户名 如果不符合正则表达式要求\r\n\r\n              //提示用户输入出错提示框\r\n              //停止当前函数执行\r\n                 if(!regu.test(u)){\r\n                   this.$messagebox(\"消息\",\"用户名格式不正确\");\r\n                   return;\r\n                 }\r\n              //验证密码 如果不符合要求的话\r\n              if(!regp.test(p)){\r\n              //提示用户密码格式不正确\r\n                  this.$messagebox(\"消息\",\"密码格式不正确\")\r\n                  return;\r\n              }\r\n              //验证年龄是否符合要求\r\n              if(!rega.test(a)){\r\n                  this.$messagebox(\"消息\",\"年龄格式不正确\");\r\n                  return;\r\n             }\r\n             //把符合要求的数据 通过ajax发给服务器\r\n             //插入语句 进行注册\r\n          }  \r\n        }\r\n   }\r\n</script>"],"sourceRoot":"src\\components\\exam"}]}